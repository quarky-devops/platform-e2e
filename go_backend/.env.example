# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Supabase Configuration (Required)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-key-here

# Server Configuration
PORT=8080
GIN_MODE=release

# Database Configuration (Optional - for local development)
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# Performance Configuration (Optional)
MAX_CONCURRENT_ASSESSMENTS=10
ASSESSMENT_TIMEOUT_SECONDS=60
HTTP_TIMEOUT_SECONDS=30

# Security Configuration (Optional)
CORS_ORIGINS=*
RATE_LIMIT_REQUESTS_PER_MINUTE=100

# Logging Configuration (Optional)
LOG_LEVEL=info
LOG_FORMAT=json

# External Services (Optional)
URLVOID_API_KEY=your-urlvoid-api-key
IPVOID_API_KEY=your-ipvoid-api-key

# Webhook Configuration (Optional)
WEBHOOK_URL=https://your-webhook-url.com/webhooks
WEBHOOK_SECRET=your-webhook-secret

# Monitoring Configuration (Optional)
PROMETHEUS_ENABLED=false
PROMETHEUS_PORT=9090
